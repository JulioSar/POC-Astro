---
import News from "../../../MOCK_DATA_FEATURED_NEWS.json";
import NewsCarousel from "./NewsCarousel";
---

<div class="border rounded-xl p-10 bg-gray-100 border-none">
  <section>
    <h1 class="text-2xl mb-20">Featured News</h1>
    <NewsCarousel items={News} client:load/>
  </section>
  <section class="mt-12">
    <a class="cursor-pointer text-sm" href="google.com">Go to News</a>
  </section>
</div>
