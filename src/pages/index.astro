---
import Layout from "../layouts/Layout.astro";
import "../../app/globals.css";
import type { User } from "@/types";
import { v4 } from "uuid";
import Dashboard from "@/components/Dashboard/Dashboard.astro";
import { Image } from "astro:assets";
import ApexLogo from "../assets/ApexLogo_.png";
//TEST
const user : User={
  id:v4(),
  name:"Julio Sarmiento",
  mail:"julio.sarmiento@apexfs.group",
  status:true,
  profile_picture:"https://avatars.git"
}
---

<Layout title="Dashboard.">
  <section class="justify-end align-middle items-end bg-white flex flex-col pr-10 pt-10">
    <Image src={ApexLogo} alt="Apex Logo" class="mb-12 h-24 w-24 sm:h-20" />
  </section>
  <Dashboard user = {user}/>
</Layout>
